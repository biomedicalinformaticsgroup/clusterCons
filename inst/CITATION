citHeader("To cite package 'clusterCons' in publications use:")
     
citEntry(
	entry="Manual",
	title = "Merged consensus clustering to assess and improve class discovery with microarray data",
	author = personList(as.person("T. Ian Simpson"),
						as.person("J. Douglas Armstrong"),
						as.person("Andrew P. Jarman")
						),
	year = 2010,
	journal = 'BMC Bioinformatics',
	volume = 11,
	pages = 590,

	textVersion = paste(
		"Merged consensus clustering to assess and improve class discovery with microarray data.",
		"Simpson TI, Armstrong JD and Jarman AP.",
		"BMC Bioinformatics 2010, 11:590",
		sep=' '
		)
)